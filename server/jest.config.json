{
    "setupFilesAfterEnv": [
        "<rootDir>/__tests__/setup.ts"
    ],
    "testPathIgnorePatterns": [
        "<rootDir>/__tests__/setup.ts"
    ],
    "moduleNameMapper": {
        "@src/(.*)": "<rootDir>/src/$1",
        "@routes/(.*)": "<rootDir>/src/routes/$1",
        "@middlewares/(.*)": "<rootDir>/src/middlewares/$1",
        "@common/(.*)": "<rootDir>/src/common/$1",
        "@services/(.*)": "<rootDir>/src/services/$1",
        "@models/(.*)": "<rootDir>/src/models/$1"
    }
}